generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model collection {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  author   String
  prompt   String
  src      String   @unique
  date     DateTime
  download Int
  views    Int
}
